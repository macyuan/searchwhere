<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/side_nav_bar"
    android:orientation="vertical">


    <ImageView
        android:id="@+id/iv_start_map"
        android:layout_width="100dp"
        android:visibility="visible"
        android:layout_height="100dp"
        android:layout_gravity="center"
        android:src="@mipmap/icon_start_map" />

    <ImageView
        android:id="@+id/iv_start_logo"
        android:visibility="invisible"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:src="@mipmap/my_launcher" />


    <TextView
        android:id="@+id/tv_start_app_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal|bottom"
        android:layout_marginBottom="@dimen/nav_header_bigicon_size"
        android:visibility="invisible"
        android:text="@string/app_name"
        android:textColor="@android:color/white"
        android:textSize="@dimen/app_name_size" />
</FrameLayout>